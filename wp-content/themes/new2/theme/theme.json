{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 1,
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [{
					"slug": "background",
					"color": "#ffffff",
					"name": "Background"
				},
				{
					"slug": "foreground",
					"color": "#484c55",
					"name": "Foreground"
				},
				{
					"slug": "blue",
					"color": "#007cba",
					"name": "Blue"
				},
				{
					"slug": "green",
					"color": "#6eaa3e",
					"name": "Green"
				},
				{
					"slug": "events",
					"color": "#02a4a4",
					"name": "Events"
				},
				{
					"slug": "vehicle",
					"color": "#436ab1",
					"name": "Vehicle"
				},
				{
					"slug": "utilities",
					"color": "#208bc5",
					"name": "Utilities"
				},
				{
					"slug": "pet",
					"color": "#e57b31",
					"name": "Pet"
				},
				{
					"slug": "senior",
					"color": "#6f69a9",
					"name": "Senior"
				},
				{
					"slug": "medical",
					"color": "#bb1f33",
					"name": "Medical"
				},
				{
					"slug": "worship",
					"color": "#0283a3",
					"name": "Worship"
				},
				{
					"slug": "preschool",
					"color": "#68b142",
					"name": "Preschool"
				},
				{
					"slug": "school",
					"color": "#914c91",
					"name": "School"
				},
				{
					"slug": "recreation",
					"color": "#2c8f4e",
					"name": "Recreation"
				},
				{
					"slug": "athletics",
					"color": "#ce4e37",
					"name": "Athletics"
				},
				{
					"slug": "activities",
					"color": "#c3569e",
					"name": "Activities"
				}
			]
		},
		"spacing": {
			"spacingSizes": [
                {
                    "name": "0.25rem",
                    "size": "0.25rem",
                    "slug": "10"
                },
                {
                    "name": "0.5rem",
                    "size": "0.5rem",
                    "slug": "20"
                },
                {
                    "name": "0.75rem",
                    "size": "0.75rem",
                    "slug": "30"
                },
                {
                    "name": "2rem clamp 0.75rem",
                    "size": "clamp(1.5rem, 0.9421rem + 1.6529vw, 2rem)",
                    "slug": "40"
                },
                {
                    "name": "5rem clamp 3rem",
                    "size": "clamp(3rem, 2.0909rem + 2.9091vw, 5rem)",
                    "slug": "50"
                },
                {
                    "name": "6rem",
                    "size": "6rem",
                    "slug": "60"
                },
                {
                    "name": "8rem clamp 1.5rem",
                    "size": "clamp(1.5rem, -0.8136rem + 8.8136vw, 8rem)",
                    "slug": "70"
                },
                {
                    "name": "14rem",
                    "size": "14rem",
                    "slug": "80"
                }
            ],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},		
		"typography": {
			"customFontSize": false,
			"lineHeight": true,
			"fontFamilies": [
				{
				"fontFamily": "\"Open Sans\", sans-serif",
				"slug": "serif",
				"name": "Serif"
				},
				{
				"fontFamily": "'Dancing Script', cursive",
				"slug": "script",
				"name": "Script"
				}
			],
			"fontSizes": [
				{
					"slug": "x-small",
					"size": "clamp(0.5rem, 0.43rem + 0.32vw, 0.75rem)",
					"name": "Extra Small"
				},
				{
					"slug": "small",
					"size": "clamp(0.75rem, 0.715rem + 0.16vw, 0.875rem)",
					"name": "Small"
				},
				{
					"slug": "medium",
					"size": "clamp(0.875rem, 0.84rem + 0.16vw, 1rem)",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "clamp(1rem, 0.9432rem + 0.1818vw, 1.125rem)",
					"name": "Large"
				},
				{
					"slug": "x-large",
					"size": "clamp(1.125rem, 1.0682rem + 0.1818vw, 1.25rem)",
					"name": "Extra Large"
				},
				{
					"slug": "2-xl",
					"size": "clamp(1.25rem, 1.1364rem + 0.3636vw, 1.5rem)",
					"name": "2xl"
				},
				{
					"slug": "3-xl",
					"size": "clamp(1.5rem, 1.3295rem + 0.5455vw, 1.875rem)",
					"name": "3xl"
				},
				{
					"slug": "4-xl",
					"size": "clamp(2rem, 1.8864rem + 0.3636vw, 2.25rem)",
					"name": "4xl"
				},
				{
					"slug": "5-xl",
					"size": "clamp(3rem, 2.5455rem + 1.4545vw, 4rem)",
					"name": "5xl"
				},
				{
					"slug": "6-xl",
					"size": "clamp(3rem, 2.3182rem + 2.1818vw, 4.5rem)",
					"name": "6xl"
				}
			]
		},
		"layout": {
			"contentSize": "1024px",
			"wideSize": "1600px"
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "0.5"
				},
				"color": {
					"background": "var(--wp--preset--color--blue)",
					"text": "var(--wp--preset--color--background)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/post-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--source-serif-pro)",
					"fontWeight": "300",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"core/post-comments": {
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--small)"
					}
				}
			},
			"core/pullquote": {
				"border": {
					"width": "1px 0"
				}
			},
			"core/query-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--source-serif-pro)",
					"fontWeight": "300",
					"lineHeight": "var(--wp--custom--typography--line-height--small)",
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"core/quote": {
				"border": {
					"width": "1px"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-font)",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontStyle": "italic",
					"fontWeight": "normal"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--sans)",
			"lineHeight": "var(--wp--custom--typography--line-height--normal)",
			"fontSize": "var(--wp--preset--font-size--medium)"
		},
		"blocks": {
			"core/paragraph": {
				"spacing": {
					"padding": true,
					"margin": true
				}
			},
			"core/heading": {
				"spacing": {
					"padding": true,
					"margin": true
				}
			},
			"core/list": {
				"spacing": {
					"padding": true,
					"margin": true
				}
			},
			"core/group": {
				"spacing": {
					"padding": true,
					"margin": true
				}
			}
		}
	}
}
